{
  "name": "Clickup Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Add task-id in title. (unofficial)",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "./background.js"
    ],
    "persistent": false
  },
  "content_scripts":[
    {
      "matches": ["https://app.clickup.com/*"],
      "js": ["./contentScripts.js"]
    }
  ]
}
